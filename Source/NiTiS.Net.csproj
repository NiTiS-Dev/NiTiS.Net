<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="$(NBT_HOME)/csharp/v1.1/init.xml" />
	<PropertyGroup>
		<NBT_Name>NiTiS.Net</NBT_Name>
		<NBT_Singleframe>false</NBT_Singleframe>
		<NBT_Version>0.0.0.3</NBT_Version>
		<NBT_Summary>
			The NiTiS library for interaction with web
		</NBT_Summary>
		
		<RepositoryUrl>https://github.com/NiTiS-Dev/NiTiS.Net</RepositoryUrl>

		<PackageLicenseUrl></PackageLicenseUrl>

		<NBT_Tabs>NiTiS;Net;Web</NBT_Tabs>
		<NBT_Frameworks>net6.0;netstandard2.1;netcoreapp3.1;net5.0</NBT_Frameworks>
		<NBT_Nullable>enable</NBT_Nullable>
	</PropertyGroup>
	
	<PropertyGroup Condition="'$(GITHUB_ACTIONS)' == 'true'">
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>
	
	<Import Project="$(NBT_HOME)/csharp/v1.1/post.xml" />
</Project>
